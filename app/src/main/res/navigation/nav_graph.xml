<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/projectPickFragment">

    <fragment
        android:id="@+id/projectPickFragment"
        android:name="com.example.appachetestwork.fragments.ProjectPickFragment"
        android:label="ProjectPickFragment" >
        <action
            android:id="@+id/action_projectPickFragment_to_drawingScreenFragment"
            app:destination="@id/drawingScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/drawingScreenFragment"
        android:name="com.example.appachetestwork.fragments.DrawingScreenFragment"
        android:label="DrawingScreenFragment" >
        <argument
            android:name="project_id"
            app:argType="string" />
    </fragment>
</navigation>